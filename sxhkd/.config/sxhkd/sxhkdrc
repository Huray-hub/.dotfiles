super + Return
  $TERMINAL

alt + space
  rofi -show 

XF86AudioMute
  pamixer -t

XF86Audio{Raise,Lower}Volume
  pamixer -{i,d} 7 

# XF86AudioRaiseVolume 
#   pamixer -i 7  

XF86Audio{Play,Pause}
  playerctl play-pause
  
XF86Audio{Next,Prev}
  playerctl {next,previous}

# Brightness controls
XF86MonBrightness{Up,Down}
  brillo -q -{A,U} 10

# XF86MonBrightnessUp
#  brillo -q -A 10


XF86Calculator
  rofi -show calc


# Screenshot
Print 
  flameshot gui

# Toggle bt headphones connection
super + k
  ~/.local/bin/bt-toggle-headphones.sh
